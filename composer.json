{
    "name": "visio/kanban",
    "description": "A FilamentPHP 3 plugin for Kanban boards and issue management",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "filament",
        "filament-plugin",
        "kanban",
        "laravel",
        "issue-management"
    ],
    "authors": [
        {
            "name": "Visiosoft",
            "email": "info@visiosoft.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Visiosoft\\Kanban\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Visiosoft\\Kanban\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Visiosoft\\Kanban\\KanbanServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
